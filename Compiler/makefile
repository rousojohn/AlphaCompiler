all:
	flex scanner.l
	bison parser.y
	gcc -o analyzer lexical_analyzer.c parser.c symbolList.c SymTable.c stack.c quad.c icode.c

nopatch:
	flex scanner.l
	bison parser-nopatch.y
	gcc -o analyzer lexical_analyzer.c parser.c symbolList.c SymTable.c stack.c quad.c icode.c

debug:
	flex scanner.l
	bison parser.y
	gcc -Wall -g -o analyzer lexical_analyzer.c parser.c symbolList.c SymTable.c stack.c quad.c icode.c
debug2:
	flex scanner.l
	bison parser-nopatch.y
	gcc -Wall -g -o analyzer lexical_analyzer.c parser.c symbolList.c SymTable.c stack.c quad.c icode.c

clear:
	
	rm lexical_analyzer.c # generated by scanner.l
	rm parser.c   # generated by parser.y
	rm parser.h
	rm *~
	rm analyzer
	rm *.b
